image_speed = 10 * 0.01 * obj_init.frame_delta;

/// PROJECTILE DIRECTION AND SPEED 
if (sprite_index = spr_reya_front){ x_dir =1 }
if (x_dir = 1) grav_dir = 180;
if (x_dir = -1) grav_dir = 0;
projectile_speed = 4 * obj_init.frame_delta;

/// STAMINA  
if(((keyboard_check(vk_shift)) &amp;&amp; (global.stamina &gt; 0)) &amp;&amp; (global.stamina_disable = false)){
    part_type_gravity(obj_first_particle.first_particle,0.05,180); //grav_dir); if dynamic switch dir
    
    if (obj_stamina_bar.brightness &gt;= 200){
    projectile_speed += 3 * obj_init.frame_delta;
    global.stamina -= 2.5;
    if (global.stamina &lt;= 0) {global.stamina_disable = true;}
    
    if (global.stamina_disable = true) {
    obj_stamina_bar.brightness=30;
    obj_stamina_bar.glow=20;
    obj_stamina_bar.glow_speed=0;
    obj_stamina_bar.sprite_index = 1; 
    }
    } // fail safe loop for brightness of stamina bar, no running
}

if((global.stamina_disable = true) &amp;&amp; (global.stamina &gt;= global.max_stamina-1)){
    alarm[0] = 1 * obj_init.frame_delta;
    obj_stamina_bar.txt_stamina_alpha = 0.01; //set
    global.stamina_disable = false;
} 

if (obj_stamina_bar.txt_stamina_alpha &gt;= 0 &amp;&amp; obj_stamina_bar.txt_stamina_alpha &lt;= 1){ 
    if (global.stamina_disable = false) obj_stamina_bar.txt_stamina_alpha += 0.02; 
}

if(!keyboard_check(vk_shift)) or (global.stamina_disable = true){
    global.stamina += 2; /// restful recovery 
}

/// PARTICLE DIRECTION 
sameDir = x_dir; 
if((keyboard_check(vk_left))){
    x_spd = -1;
    part_type_gravity(obj_first_particle.first_particle,0.02,0);
    x_dir = -1;
}

if((keyboard_check(vk_right))){
    x_spd = 1;
    part_type_gravity(obj_first_particle.first_particle,0.02,180);
    x_dir = 1;
}

/// DOUBLE JUMP CODE

if (timer &gt; -3) {timer = timer - (1*obj_init.frame_delta);}

if(timer &lt;= 0) {   
    jump_current = 0;
} 

if(keyboard_check_released(vk_space) &amp;&amp; jump_current &lt;= 0) {
    jump_start_y = y; 
    ysp = -5;
    jump_current = 1;
    timer = 35;
}
        
if((timer &gt; 0) &amp;&amp; (jump_current = 1)) {
        
        if(keyboard_check_pressed(vk_space)) {
            ysp = -3;
            timer = timer + 20; 
            jump_current = 2;
        } 
}           
            
if (timer &gt; 0) {
        
        if((timer &lt;= 13) &amp;&amp; jump_current = 1){  // first jump
            ysp = 3;  
            
            if ((y &lt; (jump_start_y-3)) &amp;&amp; (ysp &gt; 0)){
            ysp = 3;
            timer = timer + 1; 
            }           
        }
        
        if((timer &lt;= 15) &amp;&amp; jump_current = 2){  // second jump
            ysp = 4.5;
                if ((y &lt; (jump_start_y-5)) &amp;&amp; (ysp &gt; 0)){
                    ysp = 4.5;
                    timer = timer + 1; 
                }  
        }
               
        if ((y &gt; jump_start_y) &amp;&amp; (ysp &gt; 0)) {
            timer = 0; 
            ysp = 0; 
            y = jump_start_y;
        } 
        
        y += ysp * obj_init.frame_delta;
}

// GRAVITY
if((!place_meeting(x, y + ysp, obj_solid)) &amp;&amp; (jump_current &lt;= 0)){
    y += 4 * obj_init.frame_delta;;
}

/// RUNNING
if (keyboard_check(vk_shift)) &amp;&amp; (global.stamina_disable = false) {
    if (obj_stamina_bar.brightness &gt;= 200){ //&amp;&amp; (obj_stamina_bar.txt_stamina_alpha &lt;= 0.00){
    spd = 7; 
    image_speed = 17 * 0.01 * obj_init.frame_delta; 
    }
}

if (keyboard_check(vk_right)) {
    x += spd; 
    sprite_index = spr_reya_right;
}

if (keyboard_check(vk_left)) {
    x -= spd; 
    sprite_index = spr_reya_left;
}

if (keyboard_check(vk_down)) {
    y += spd; 
    sprite_index = spr_reya_front;
}

if ((keyboard_check(vk_up)) &amp;&amp; (place_meeting(x, y - 5, obj_solid))) {
    y -= spd; 
    sprite_index = spr_reya_back;
}

/// Set Back to Default, if not Running
    spd = 3; 

if (hp &lt;= 0) {
    //instance_create(x-400,y-600,obj_black_fade_out); 
    alarm[5] = 30; 
}

